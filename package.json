{
  "name": "designportfolio",
  "version": "1.0.0",
  "description": "personal website using graphql and nuxt",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "precommit": "npm run precommit --prefix nuxt && npm run precommit --prefix docs && git add -A",
    "init": "npm i && cd nuxt && npm i && cd ../docs && npm i && cd ..",
